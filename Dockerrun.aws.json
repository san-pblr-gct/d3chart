{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "parthi22/d3chart-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "parthi22/d3chart-api",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "parthi22/d3chart-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client", "server"
            ]
        }
    ]
}